package hiding;

public class MyDate {
//	int day;
//	int month;
//	int year;  default 접근 제어자 -> 같은 package 내에서 public처럼 사용 가능
	
	private int day;
	private int month;
	private int year;
	
	public void setDay(int day) {
		this.day = day;
	}
	
	public void setMonth(int Month) {
		this.Month = Month;
	}
	
	public void setYear(int year) {
		this.year = year;
	}
}
